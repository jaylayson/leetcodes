class Solution {
	public static int singleNumber(int[] nums) {
	    int ans = 0;
	    for(int i = 0; i<nums.length; i++) {
	    	ans ^= nums[i]; //xor bit manipulation
	    }

//equivalent to for-loop
//	    for(int i : nums) {
//	        ans ^= i;
//	    }
        
        /*In this section implement this function using
            a data structure such as map.
        
        
        
        
        
        */
	    return ans;
	}//end of singleNumber
}
